local function autoRankUp()
    spawn(function()
        while getgenv().autorankup == true do
            for i = 1, 99 do
                game:GetService("ReplicatedStorage").Remotes.BuyRank:FireServer(i)
                wait(0)
                
                if getgenv().autorankup == false then
                    break
                end
            end
        end
    end)
end

