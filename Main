            game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("PetAction"):InvokeServer(unpack(args))
        
            wait(0)
            
            if getgenv().autocraftall == false then
                break
            end
        end
    end)
end

